    "@types/node-rsa": "^1.1.1",
    "@types/sequelize": "^4.28.10",
    "@types/sharp": "^0.28.5",
    "@types/uuid": "^8.3.1",
    "jest": "^27.0.6"
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
