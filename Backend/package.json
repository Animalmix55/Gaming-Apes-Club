{
  "name": "@gac/backend",
  "version": "1.0.0",
  "description": "The backend monorepo for GAC",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "author": "Cory Cherven",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Animalmix55/FreeFlow.git"
  },
  "scripts": {
    "start:dev": "nodemon --exec \"yarn start:fresh\"",
    "start:fresh": "lerna run build --stream && lerna run start:fresh --stream && 1",
    "start": "lerna run start --stream",
    "transpile": "lerna run transpile",
    "clean": "lerna run clean",
    "build": "lerna run build --stream",
    "server": "lerna run server",
    "dev": "lerna run dev",
    "prod": "lerna run prod",
    "watch:dev": "lerna run watch:dev",
    "test": "jest",
    "bootstrap": "lerna bootstrap",
    "heroku:push": "cd .. && git subtree push --prefix Backend heroku master && cd Backend"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@types/concat-stream": "^1.6.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express-jwt": "^6.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node-rsa": "^1.1.1",
    "@types/uuid": "^8.3.1",
    "concat-stream": "^2.0.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-jwt": "^6.1.0",
    "jest": "^27.0.6",
    "jsonwebtoken": "^8.5.1",
    "kill-port": "^1.6.1",
    "lerna": "^4.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "node-rsa": "^1.1.1",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "web3": "^1.6.0",
    "discord.js": "^13.6.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^2.4.1"
  }
}
